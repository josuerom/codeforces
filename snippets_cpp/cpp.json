			"ios::sync_with_stdio(false); cin.tie(nullptr); cout.tie(nullptr);\n"
		]
	},

	"using namespace std;": {
		"prefix": "u",
		"body": [
			"using namespace std;"
		]
	},

	"return": {
		"prefix": "re",
		"body": [
			"return$1;"
		]
	},

	"char or string getline()": {
		"prefix": "readline",
		"body": [
			"cin.getline($1, ${2:size}, '\\n');"
		]
	},

	"for (int i = 0; i < n; i++)": {
		"prefix": "fori",
		"body": [
			"for (int ${1:i} = 0; ${1:i} < ${2:n}; ${1:i}++) {",
			"}",
		]
	},

	"for (auto $e : vec)": {
		"prefix": "fore",
		"body": [
			"for (auto &e : ${vector}) {",
			"}",
		]
	},

	"#include <bits/stdc++.h>": {
		"prefix": "in",
		"body": [
			"#include <${1:bits/stdc++.h}>"
		]
	},

	"typedef": {
		"prefix": "typ",
		"body": [
			"typedef ${1:long long ll};"
		]
	},

	"#define": {
		"prefix": "de",
		"body": [
			"#define $1"
		]
	},

	"void solve() { body }": {
		"prefix": "solve",
		"body": [
			"void solve$1() {",
			"}"
		]
	},

	"function main": {
		"prefix": "m",
		"body": [
			"int main() {",
			"\t$1",
			"\treturn 0;",
			"}"
		]
	},

