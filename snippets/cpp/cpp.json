      "void$3 solve() {",
      "}\n",
      "int main() {",
      "\tios::sync_with_stdio(false);",
      "\tcin.tie(nullptr); cout.tie(nullptr);",
      "\tint tt = 1;",
      "\tcin >> tt;$1",
