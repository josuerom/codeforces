	"return": {
		"prefix": "re",
		"body": [
			"return $1;"
		]
	},

	"read char[]": {
		"prefix": "rdchar",
		"body": [
			"cin.getline($1, ${2:size}, '\\n');"
		]
	},

	"read string": {
		"prefix": "rdstr",
		"body": [
			"getline(cin, ${1:string});"
		]
	},

	"for (int i = 0; i < n; i++)": {
		"prefix": "fori",
		"body": [
			"for (int ${1:i} = 0; ${1:i} < ${2:n}; ${1:i}++) {",
			"}",
		]
	},

	"for (auto $e : vec)": {
		"prefix": "fore",
		"body": [
			"for (auto &e : ${1:vector}) cin >> e;",
		]
	},

	"while tests case": {
		"prefix": "tt",
		"body": [
			"int tt;",
			"cin >> tt;",
			"while (tt-- > 0) {",
			"\t$1",
			"}",
		]
	},

	"#include <bits/stdc++.h>": {
		"prefix": "ii",
